        section.pricing-area.shortcode-item
          .container
            h2 Pricing Table
            .row.text-center
              .col-sm-4.plan.price-one.wow.fadeInDown
                ul
                  li.heading-one
                    h3 Start Up
                    span $6/Month
                  li 5 Gb Disk Space
                  li 1GB Dadicated Ram
                  li 10 Addon Domain
                  li 10 Email Account
                  li 24/7 Support
                  li.plan-action
                    a.btn.btn-primary(href='') Sign up
              .col-sm-4.plan.price-two.wow.fadeInDown
                ul
                  li.heading-two
                    h3 Standerd
                    span $12/Month
                  li 10 Gb Disk Space
                  li 2GB Dadicated Ram
                  li 20 Addon Domain
                  li 20 Email Account
                  li 24/7 Support
                  li.plan-action
                    a.btn.btn-primary(href='') Sign up
              .col-sm-4.plan.price-three.wow.fadeInDown
